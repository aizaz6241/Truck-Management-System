{
  "name": "truck-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate && prisma db push",
    "backup": "npx tsx scripts/backup.ts",
    "restore": "npx tsx scripts/restore.ts",
    "db:push:dev": "dotenv -e .env.development -- prisma db push",
    "db:push:prod": "dotenv -e .env.production -- prisma db push",
    "studio:dev": "dotenv -e .env.development -- prisma studio",
    "studio:prod": "dotenv -e .env.production -- prisma studio",
    "generate:dev": "dotenv -e .env.development -- prisma generate",
    "generate:prod": "dotenv -e .env.production -- prisma generate"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "5.10.2",
    "@uploadthing/react": "^7.3.3",
    "bcryptjs": "^3.0.3",
    "dotenv": "^17.2.3",
    "jose": "^6.1.3",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "lucide-react": "^0.563.0",
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "^3.6.0",
    "uploadthing": "^7.7.4"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.23",
    "babel-plugin-react-compiler": "1.0.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "postcss": "^8.5.6",
    "prisma": "5.10.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5"
  }
}
